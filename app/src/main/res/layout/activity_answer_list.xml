<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="comuiappcenter.facebook.m.legacy.AnswerListActivity">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/InnerRelative1_answerList">

        <comuiappcenter.facebook.m.legacy.defaultTextBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/title_box_answerList"
            app:minLines="1"

            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/InnerRelative1_answerList"
        android:layout_centerHorizontal="true"
        android:id="@+id/InnerRelative2_answerList"
        android:layout_marginTop="10dp">

        <comuiappcenter.facebook.m.legacy.defaultTextBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/body_box_answerList"
            app:minLines="4"
            />
    </RelativeLayout>
</RelativeLayout>
